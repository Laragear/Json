{
    "name": "laragear/json",
    "description": "Easily retrieve and manipulate `Json` across your application.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "json",
        "http"
    ],
    "authors": [
        {
            "name": "Italo Israel Baeza Cabrera",
            "email": "darkghosthunter@gmail.com",
            "homepage": "https://github.com/sponsors/DarkGhostHunter"
        }
    ],
    "support": {
        "source": "https://github.com/Laragear/Json",
        "issues": "https://github.com/Laragear/Json/issues"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "10.*|11.*",
        "illuminate/http": "10.*|11.*"
    },
    "require-dev": {
        "orchestra/testbench": "8.*|9.*"
    },
    "autoload": {
        "psr-4": {
            "Laragear\\Json\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laragear\\Json\\JsonServiceProvider"
            ]
        }
    },
    "funding": [
        {
            "type": "GitHub Sponsorship",
            "url": "https://github.com/sponsors/DarkGhostHunter"
        },
        {
            "type": "Paypal",
            "url": "https://paypal.me/darkghosthunter"
        }
    ]
}
